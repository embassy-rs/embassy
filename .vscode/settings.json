    // "examples/stm32f0/Cargo.toml",
    // "examples/stm32f1/Cargo.toml",
    // "examples/stm32f2/Cargo.toml",
    // "examples/stm32f3/Cargo.toml",
    // "examples/stm32f334/Cargo.toml",
    // "examples/stm32f4/Cargo.toml",
